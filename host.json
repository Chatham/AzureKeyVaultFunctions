{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
      "default": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "includedTypes": "Event"
      },
      "enableLiveMetrics": false,
      "enableDependencyTracking": false,
      "enablePerformanceCountersCollection": false,            
      "snapshotConfiguration": {
        "isEnabled": false
      }
    },
    "console": {
      "isEnabled": true
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[1.*, 2.0.0)"
  },
  "managedDependency": {
    "enabled": true
  }
}
